package com.kshl.huihuimall.portal.controller.request;

import com.kshl.base.reqeust.BasePageRequest;


/**
 * 分润请求
 */
public class ProfitRequest extends BasePageRequest {
    //门店id
    private Integer storeId;
    //时间
    private  String date;
    //订单号
    private  String orderNumber;
    //支付状态
    private  Integer status;

    public Integer getStatus() {
        return status;
    }

    public void setStatus(Integer status) {
        this.status = status;
    }

    // 推荐门店
    private Integer reStoreId;

    public Integer getReStoreId() {
        return reStoreId;
    }

    public void setReStoreId(Integer reStoreId) {
        this.reStoreId = reStoreId;
    }

    public Integer getStoreId() {
        return storeId;
    }

    public void setStoreId(Integer storeId) {
        this.storeId = storeId;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }

    public String getOrderNumber() {
        return orderNumber;
    }

    public void setOrderNumber(String orderNumber) {
        this.orderNumber = orderNumber;
    }

}
