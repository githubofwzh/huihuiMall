package com.kshl.huihuimall.console.base.pojo;

import com.kshl.huihuimall.base.dal.entity.Device;
import com.kshl.huihuimall.base.dal.entity.Store;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 订单商品信息返回pojo
 */
public class OrderProductPojo implements Serializable {
    //门店id
    private Integer storeId;
    //订单利润
    private BigDecimal profit;

    public Integer getStoreId() {
        return storeId;
    }

    public void setStoreId(Integer storeId) {
        this.storeId = storeId;
    }

    public BigDecimal getProfit() {
        return profit;
    }

    public void setProfit(BigDecimal profit) {
        this.profit = profit;
    }

    @Override
    public String toString() {
        return "OrderProductPojo{" +
                "storeId=" + storeId +
                ", profit=" + profit +
                '}';
    }
}