package com.kshl.huihuimall.console.base.pojo;

import com.kshl.huihuimall.base.dal.entity.ReturnExchangeOrder;
import com.kshl.huihuimall.base.dal.entity.StoreOrder;

import java.io.Serializable;

/**
 * 退换货详情对象
 */
public class ReturnExchangePojo implements Serializable {

  //订单对象
  private StoreOrder storeOrder;

  //退换货对象
    private ReturnExchangeOrder returnExchangeOrder;

    public StoreOrder getStoreOrder() {
        return storeOrder;
    }

    public void setStoreOrder(StoreOrder storeOrder) {
        this.storeOrder = storeOrder;
    }

    public ReturnExchangeOrder getReturnExchangeOrder() {
        return returnExchangeOrder;
    }

    public void setReturnExchangeOrder(ReturnExchangeOrder returnExchangeOrder) {
        this.returnExchangeOrder = returnExchangeOrder;
    }

    @Override
    public String toString() {
        return "ReturnExchangePojo{" +
                "storeOrder=" + storeOrder +
                ", returnExchangeOrder=" + returnExchangeOrder +
                '}';
    }
}