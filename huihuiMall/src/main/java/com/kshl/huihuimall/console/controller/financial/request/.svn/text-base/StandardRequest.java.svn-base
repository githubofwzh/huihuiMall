package com.kshl.huihuimall.console.controller.financial.request;

import com.kshl.base.reqeust.BasePageRequest;
import java.math.BigDecimal;


/**
 * 交易对账记录请求（条件）
 */
public class StandardRequest extends BasePageRequest {
    //id
    private Integer id;
    //门店分润比例
    private BigDecimal storeProfit;
    //推荐分润比例
    private BigDecimal recommendProfit;
    //消费分润比例
    private BigDecimal consumptionProfit;


    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public BigDecimal getStoreProfit() {
        return storeProfit;
    }

    public void setStoreProfit(BigDecimal storeProfit) {
        this.storeProfit = storeProfit;
    }

    public BigDecimal getRecommendProfit() {
        return recommendProfit;
    }

    public void setRecommendProfit(BigDecimal recommendProfit) {
        this.recommendProfit = recommendProfit;
    }

    public BigDecimal getConsumptionProfit() {
        return consumptionProfit;
    }

    public void setConsumptionProfit(BigDecimal consumptionProfit) {
        this.consumptionProfit = consumptionProfit;
    }

    @Override
    public String toString() {
        return "StandardRequest{" +
                "id=" + id +
                ", storeProfit=" + storeProfit +
                ", recommendProfit=" + recommendProfit +
                ", consumptionProfit=" + consumptionProfit +
                '}';
    }
}
