    // 订单号
    private String orderNumber;

    // 下单日期
    private Date startTime;

    // 门店名称
    private String storeName;

    // 门店地址
    private String storeAddress;

    // 订单价总和
    private BigDecimal orderTotal;

    // 出库操作人
    private String createUser;

    private String deviceCode;
    private String shopkeeperPhone;
    //打印从系统获取
    private String czName;
    private String czTime;

    public String getCzName() {
        return czName;
    }

    public void setCzName(String czName) {
        this.czName = czName;
    }

    public String getCzTime() {
        return czTime;
    }

    public void setCzTime(String czTime) {
        this.czTime = czTime;
    }

    public String getShopkeeperPhone() {
        return shopkeeperPhone;
    }

    public void setShopkeeperPhone(String shopkeeperPhone) {
        this.shopkeeperPhone = shopkeeperPhone;
    }

    public String getDeviceCode() {
        return deviceCode;
    }

    public void setDeviceCode(String deviceCode) {
        this.deviceCode = deviceCode;
    }

    public String getCreateUser() {
        return createUser;
    }

    public void setCreateUser(String createUser) {
        this.createUser = createUser;
    }

