ul li{
    list-style: none;
    float: left;
    margin-bottom: 0px;
}
a:active{
    text-decoration: none;
}
.search{
    padding: 7px 18px 7px 14px;
    border-bottom: 1px solid #e1e1e1;
    background-color: #FFFFFF;
}
.search_email{
    height: 30px;
    border: 0;
    background-color: #f5f5f5;
    border-radius: 6px;
    outline: 0;
    font-size: 13px;
    font-family: PingFang-SC-Regular;
    color: #aaaaaa;
    background-image: url(../../../../images/frontpages/common/soso.png) ;
    background-repeat: no-repeat;
    background-position: 12px 7px;
    background-size: 15px;
    padding-left: 32px;
}
.tab_title{
    padding: 10px;
    font-size: 14px;
    font-family: PingFang-SC-Reguiar;
}
.tab_title:hover{
    border-bottom: 3px solid #c57030;
    margin-bottom: 0;
}
.tab_title:active{
    border-bottom: 3px solid #c57030;
    margin-bottom: 0;
}
#Liactive{
    border-bottom: 3px solid #c57030;
    margin-bottom: 0;
}


.ul_main{
    border-bottom: 1px solid #dddddd;
    background-color: #FFFFFF;
}
.button{
    padding: 5px 8px;
    height:30px;
    background-color: #c57030;
    border-radius: 4px;
    border: 0;
    color: #fefeff;
    font-size: 14px;
    font-family: PingFang-SC-Reguiar;
    float:right;
}

.number{
    padding: 8px;
    background-color: #fffbef;
}
.number span{
    font-size: 15px;
    color: #333333;
    font-family: PingFang-SC-Reguiar;
    margin-left: 10px;
    line-height: 32px;
    font-weight: bold;
}

.time{
    padding: 15px;
    border-top: 1px solid #ecebe7;
    border-bottom: 1px solid #ecebe7;
    font-size: 14px;
    background-color: #FFFFFF;
}
.time span{
    font-size: 14px;
    color: #666666;
    font-family: PingFang-SC-Reguiar;
}
.time span:nth-child(1){
    margin-right: 7px;
}
.black{
    background-color: #f5f5f5;
    height: 10px;
}
.time .Goods_receipt{
    float: right;
    color: #c78360;
    font-size: 14px;
    font-family: PingFang-SC-Medium;
}
.time .complete{
    float: right;
    color: #333333;
    font-size: 14px;
    font-family: PingFang-SC-Medium;
}
.time .Arrive{
    float: right;
    color: #228efa;
    font-size: 14px;
    font-family: PingFang-SC-Medium;
}
