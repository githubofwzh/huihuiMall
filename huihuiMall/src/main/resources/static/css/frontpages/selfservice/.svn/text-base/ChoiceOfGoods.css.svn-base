html,body{
    margin: 0;
    padding: 0;
}
.brand-panel {
    padding: 10px 0px 10px 0px;
    background-color: #FFFFFF;
    overflow-x:hidden ;
    overflow-y: auto;
}

.brand-title {
    padding: 15px 0px;
    text-align: center;
    vertical-align: middle;
}

.brand-title span{
    font-size: 24px;
    color: #b75816;
    font-family: "microsoft yahei";
}

ul,li{
    list-style: none;
    padding: 3px 0px;
    margin-bottom: 0px;
}
li{
    background-color: #FFFFFF !important;
    font-size: 16px;
    color: #c57030;
    cursor: pointer;
}
.brand-item{
    text-align: center;
    width: 74px;
    height: 28px;
    background-color: #f5f5f5;
    border: 1px solid #b9541c;
    border-radius: 6px;
}

.brand-item span{
    line-height: 32px;
    font-size: 18px;
}

.active{
    background-color: #c57030 !important;
    color: #ffffff;
}
.swiper-button-prev{
    position: relative !important;
    top: 17px !important;
}
.swiper-button-next{
    position: relative !important;
    top: 17px !important;
    left: 30px !important;
}
.theme{
    padding: 100px 0px;
    background-color: #FFFFFF;
    height: auto;
    overflow:hidden;
}
.paddingZero{
    padding:0;
}
.shopImg img{
    width: 250px;
    height: 250px;
}
.shop_money{
    color: #f33838 !important;
    font-size: 22px !important;
    margin: 8px 0 0 0;

}
.shop_money i,.shop_money span{
    font-size: 14px !important;
}
.shopDetails{
    margin: 0;
    font-size: 14px;
    color: #333333;
    font-family: "microsoft yahei";
}
.shop_name{
    margin: 0;
    font-size: 20px;
    color: #333333;
    padding-bottom: 7px;
    height: 56px;
    font-family: "microsoft yahei";
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp:2;
    -webkit-box-orient: vertical;
}
.type_sp{
    margin: 0;
    border: 1px solid #b75816;
    font-size: 11px;
    color: #b75816;
    display: inline-block;
    text-align: center;
    border-radius: 3px;
}
.shop_main{
    margin-left: 1%;
    margin-right: 2%;
    margin-top: 2%;
}
.CommodityDetails{
    padding: 0 55px;
}
.theme_title{
    padding: 18px 0px;
    border-bottom: 1px solid #f6f6f4;
}
.search{
    padding: 10px 0px;
    vertical-align: middle;
}
